{
  "expo": {
    "name": "cybcom",
    "slug": "cybcom-app",
    "version": "1.0.0",
    "orientation": "portrait",
    
    // 1. Ícone Principal (PNG é o ideal)
    "icon": "./assets/images/logo-cybcomPNG.png",
    
    "scheme": "cybcomapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        // 2. Ícone Adaptativo
        "foregroundImage": "./assets/images/logo-cybcomPNG.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ewayrton.cybcomapp"
    },
    "web": {
      "output": "static",
      // 3. Favicon
      "favicon": "./assets/images/logo-cybcomPNG.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          // 4. Splash Screen (Tela de abertura)
          "image": "./assets/images/logo-cybcomPNG.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4ee7ce8f-d5a7-4661-b5c2-18ddc0c87aae"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/4ee7ce8f-d5a7-4661-b5c2-18ddc0c87aae"
    }
  }
}